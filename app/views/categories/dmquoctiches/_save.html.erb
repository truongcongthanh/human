$("ul.errors").html("")
<% if @quocgium.errors.any? %>
    <% @quocgium.errors.full_messages.each do |message| %>
        $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
    <% end %>
<% else %>
    $(".form-index").html("<%= escape_javascript(render 'table_quocgia') %>")
    $("#form-modal").modal("hide")
<% end %>